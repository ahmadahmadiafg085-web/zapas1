# Run as Admin
$wrapper = "C:\DevTools\build_to_exe.ps1"
if (-not (Test-Path $wrapper)) { Write-Error "Wrapper not found: $wrapper"; exit 1 }

function Add-Ctx($ext) {
    $key = "Registry::HKEY_CLASSES_ROOT\$ext\shell\BuildToExe"
    if (-not (Test-Path $key)) {
        New-Item -Path $key -Force | Out-Null
    }
    New-ItemProperty -Path $key -Name "(Default)" -Value "Build to EXE" -Force | Out-Null
    New-Item -Path ($key + "\command") -Force | Out-Null
    $cmd = "powershell -ExecutionPolicy Bypass -File `"$wrapper`" `"%1`""
    New-ItemProperty -Path ($key + "\command") -Name "(Default)" -Value $cmd -Force | Out-Null
    Write-Host "Added context menu for $ext"
}

# add for .py, .ps1, .bat
Add-Ctx ".py"
Add-Ctx ".ps1"
Add-Ctx ".bat"

Write-Host "Context menu registered. Right-click a .py/.ps1/.bat file and choose 'Build to EXE'." -ForegroundColor Green
</span></code></div></div></pre>
