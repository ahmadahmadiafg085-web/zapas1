REM universal build_to_exe.bat
setlocal enabledelayedexpansion

</span><span><span class="hljs-keyword">if</span></span><span> </span><span><span class="hljs-string">"%~1"</span></span><span>==</span><span><span class="hljs-string">""</span></span><span> (
  </span><span><span class="hljs-built_in">echo</span></span><span> [خطا] هيچ فايلي انتخاب نشده.
  pause
  </span><span><span class="hljs-built_in">exit</span></span><span> /b 1
)

</span><span><span class="hljs-built_in">set</span></span><span> </span><span><span class="hljs-string">"SRC=%~1"</span></span><span>
</span><span><span class="hljs-built_in">set</span></span><span> </span><span><span class="hljs-string">"EXT=%~x1"</span></span><span>
</span><span><span class="hljs-built_in">set</span></span><span> </span><span><span class="hljs-string">"BASENAME=%~n1"</span></span><span>
</span><span><span class="hljs-built_in">set</span></span><span> </span><span><span class="hljs-string">"DIR=%~dp1"</span></span><span>
</span><span><span class="hljs-built_in">set</span></span><span> </span><span><span class="hljs-string">"DIST=%DIR%dist\%BASENAME%"</span></span><span>
</span><span><span class="hljs-keyword">if</span></span><span> not exist </span><span><span class="hljs-string">"%DIST%"</span></span><span> </span><span><span class="hljs-built_in">mkdir</span></span><span> </span><span><span class="hljs-string">"%DIST%"</span></span><span>

</span><span><span class="hljs-built_in">echo</span></span><span> ========================================
</span><span><span class="hljs-built_in">echo</span></span><span> Building: %SRC%
</span><span><span class="hljs-built_in">echo</span></span><span> ========================================

REM ---------- Python ----------
</span><span><span class="hljs-keyword">if</span></span><span> /I </span><span><span class="hljs-string">"%EXT%"</span></span><span>==</span><span><span class="hljs-string">".py"</span></span><span> (
  py -3 -m pip install --user --upgrade pip &gt;nul 2&gt;&amp;1
  py -3 -m pip install --user pyinstaller &gt;nul 2&gt;&amp;1
  pyinstaller --onefile </span><span><span class="hljs-string">"%SRC%"</span></span><span> --distpath </span><span><span class="hljs-string">"%DIST%"</span></span><span> --workpath </span><span><span class="hljs-string">"%TEMP%\pyinst_work"</span></span><span> --specpath </span><span><span class="hljs-string">"%TEMP%\pyinst_spec"</span></span><span> &gt;</span><span><span class="hljs-string">"%DIST%\build.log"</span></span><span> 2&gt;&amp;1
  </span><span><span class="hljs-keyword">if</span></span><span> errorlevel 1 (
    </span><span><span class="hljs-built_in">echo</span></span><span> [خطا] PyInstaller failed. See %DIST%\build.log
    pause
  ) </span><span><span class="hljs-keyword">else</span></span><span> (
    </span><span><span class="hljs-built_in">echo</span></span><span> ✅ Output: %DIST%\%BASENAME%.exe
    start explorer </span><span><span class="hljs-string">"%DIST%"</span></span><span>
  )
  </span><span><span class="hljs-built_in">exit</span></span><span> /b 0
)

REM ---------- PowerShell ----------
</span><span><span class="hljs-keyword">if</span></span><span> /I </span><span><span class="hljs-string">"%EXT%"</span></span><span>==</span><span><span class="hljs-string">".ps1"</span></span><span> (
  powershell -NoProfile -ExecutionPolicy Bypass -Command ^
    </span><span><span class="hljs-string">"Try { if (-not (Get-Module -ListAvailable -Name ps2exe)) { Install-Module -Name ps2exe -Scope CurrentUser -Force } ; Import-Module ps2exe -ErrorAction Stop; Invoke-ps2exe -inputFile '%SRC%' -outputFile '%DIST%\\%BASENAME%.exe' -noConsole } Catch { Write-Error <span class="hljs-variable">$_</span></span></span><span> }" &gt;</span><span><span class="hljs-string">"%DIST%\build.log"</span></span><span> 2&gt;&amp;1
  </span><span><span class="hljs-keyword">if</span></span><span> errorlevel 1 (
    </span><span><span class="hljs-built_in">echo</span></span><span> [خطا] ps2exe failed. See %DIST%\build.log
    pause
  ) </span><span><span class="hljs-keyword">else</span></span><span> (
    </span><span><span class="hljs-built_in">echo</span></span><span> ✅ Output: %DIST%\%BASENAME%.exe
    start explorer </span><span><span class="hljs-string">"%DIST%"</span></span><span>
  )
  </span><span><span class="hljs-built_in">exit</span></span><span> /b 0
)

REM ---------- Batch ----------
</span><span><span class="hljs-keyword">if</span></span><span> /I </span><span><span class="hljs-string">"%EXT%"</span></span><span>==</span><span><span class="hljs-string">".bat"</span></span><span> (
  </span><span><span class="hljs-keyword">if</span></span><span> exist </span><span><span class="hljs-string">"%ProgramFiles%\7-Zip\7z.exe"</span></span><span> (
    </span><span><span class="hljs-built_in">set</span></span><span> </span><span><span class="hljs-string">"TMP=%TEMP%\sfx_%BASENAME%"</span></span><span>
    </span><span><span class="hljs-keyword">if</span></span><span> exist </span><span><span class="hljs-string">"%TMP%"</span></span><span> rd /s /q </span><span><span class="hljs-string">"%TMP%"</span></span><span>
    </span><span><span class="hljs-built_in">mkdir</span></span><span> </span><span><span class="hljs-string">"%TMP%"</span></span><span>
    copy </span><span><span class="hljs-string">"%SRC%"</span></span><span> </span><span><span class="hljs-string">"%TMP%"</span></span><span> &gt;nul
    </span><span><span class="hljs-built_in">pushd</span></span><span> </span><span><span class="hljs-string">"%TMP%"</span></span><span>
    </span><span><span class="hljs-string">"%ProgramFiles%\7-Zip\7z.exe"</span></span><span> a -t7z payload.7z * &gt;</span><span><span class="hljs-string">"%DIST%\7z.log"</span></span><span> 2&gt;&amp;1
    </span><span><span class="hljs-built_in">set</span></span><span> </span><span><span class="hljs-string">"SFX=%ProgramFiles%\7-Zip\7z.sfx"</span></span><span>
    </span><span><span class="hljs-built_in">echo</span></span><span> ;!@Install@!UTF-8!&gt;</span><span><span class="hljs-string">"config.txt"</span></span><span>
    </span><span><span class="hljs-built_in">echo</span></span><span> RunProgram=</span><span><span class="hljs-string">"%~nx1"</span></span><span>&gt;&gt;</span><span><span class="hljs-string">"config.txt"</span></span><span>
    </span><span><span class="hljs-built_in">echo</span></span><span> ;!@InstallEnd@!&gt;&gt;</span><span><span class="hljs-string">"config.txt"</span></span><span>
    copy /b </span><span><span class="hljs-string">"%SFX%"</span></span><span>+ </span><span><span class="hljs-string">"config.txt"</span></span><span> + </span><span><span class="hljs-string">"payload.7z"</span></span><span> </span><span><span class="hljs-string">"%DIST%\%BASENAME%.exe"</span></span><span> &gt;nul
    </span><span><span class="hljs-built_in">popd</span></span><span>
    rd /s /q </span><span><span class="hljs-string">"%TMP%"</span></span><span>
    </span><span><span class="hljs-built_in">echo</span></span><span> ✅ Output: %DIST%\%BASENAME%.exe
    start explorer </span><span><span class="hljs-string">"%DIST%"</span></span><span>
    </span><span><span class="hljs-built_in">exit</span></span><span> /b 0
  ) </span><span><span class="hljs-keyword">else</span></span><span> (
    </span><span><span class="hljs-built_in">echo</span></span><span> [خطا] 7-Zip not found. Install 7-Zip.
    pause
    </span><span><span class="hljs-built_in">exit</span></span><span> /b 2
  )
)

REM ---------- Node.js (JavaScript) - pkg ----------
</span><span><span class="hljs-keyword">if</span></span><span> /I </span><span><span class="hljs-string">"%EXT%"</span></span><span>==</span><span><span class="hljs-string">".js"</span></span><span> (
  </span><span><span class="hljs-built_in">where</span></span><span> /q pkg
  </span><span><span class="hljs-keyword">if</span></span><span> errorlevel 1 (
    </span><span><span class="hljs-built_in">echo</span></span><span> pkg not found, trying to install globally...
    npm install -g pkg &gt;</span><span><span class="hljs-string">"%DIST%\npm_install.log"</span></span><span> 2&gt;&amp;1
  )
  pkg </span><span><span class="hljs-string">"%SRC%"</span></span><span> --targets node18-win-x64 --out-path </span><span><span class="hljs-string">"%DIST%"</span></span><span> &gt;</span><span><span class="hljs-string">"%DIST%\build.log"</span></span><span> 2&gt;&amp;1
  </span><span><span class="hljs-keyword">if</span></span><span> errorlevel 1 (
    </span><span><span class="hljs-built_in">echo</span></span><span> [خطا] pkg failed. See %DIST%\build.log
    pause
  ) </span><span><span class="hljs-keyword">else</span></span><span> (
    </span><span><span class="hljs-built_in">echo</span></span><span> ✅ Output: %DIST%\*.exe
    start explorer </span><span><span class="hljs-string">"%DIST%"</span></span><span>
  )
  </span><span><span class="hljs-built_in">exit</span></span><span> /b 0
)

REM ---------- TypeScript (.ts) -&gt; tsc + pkg ----------
</span><span><span class="hljs-keyword">if</span></span><span> /I </span><span><span class="hljs-string">"%EXT%"</span></span><span>==</span><span><span class="hljs-string">".ts"</span></span><span> (
  </span><span><span class="hljs-built_in">where</span></span><span> /q tsc
  </span><span><span class="hljs-keyword">if</span></span><span> errorlevel 1 (
    </span><span><span class="hljs-built_in">echo</span></span><span> tsc not found. Installing typescript...
    npm install -g typescript &gt;</span><span><span class="hljs-string">"%DIST%\npm_install.log"</span></span><span> 2&gt;&amp;1
  )
  tsc </span><span><span class="hljs-string">"%SRC%"</span></span><span> --outDir </span><span><span class="hljs-string">"%TEMP%\ts_build"</span></span><span> &gt;</span><span><span class="hljs-string">"%DIST%\tsc.log"</span></span><span> 2&gt;&amp;1
  </span><span><span class="hljs-keyword">if</span></span><span> errorlevel 1 (
    </span><span><span class="hljs-built_in">echo</span></span><span> [خطا] tsc failed. See %DIST%\tsc.log
    pause
    </span><span><span class="hljs-built_in">exit</span></span><span> /b 1
  )
  REM find compiled js (same name)
  </span><span><span class="hljs-built_in">set</span></span><span> </span><span><span class="hljs-string">"JSFILE=%TEMP%\ts_build\%BASENAME%.js"</span></span><span>
  </span><span><span class="hljs-keyword">if</span></span><span> exist </span><span><span class="hljs-string">"%JSFILE%"</span></span><span> (
    pkg </span><span><span class="hljs-string">"%JSFILE%"</span></span><span> --targets node18-win-x64 --out-path </span><span><span class="hljs-string">"%DIST%"</span></span><span> &gt;</span><span><span class="hljs-string">"%DIST%\build.log"</span></span><span> 2&gt;&amp;1
    </span><span><span class="hljs-keyword">if</span></span><span> errorlevel 1 ( </span><span><span class="hljs-built_in">echo</span></span><span> [خطا] pkg failed ) </span><span><span class="hljs-keyword">else</span></span><span> ( </span><span><span class="hljs-built_in">echo</span></span><span> ✅ Output: %DIST%\*.exe &amp; start explorer </span><span><span class="hljs-string">"%DIST%"</span></span><span> )
  ) </span><span><span class="hljs-keyword">else</span></span><span> (
    </span><span><span class="hljs-built_in">echo</span></span><span> [خطا] Compiled JS not found.
  )
  </span><span><span class="hljs-built_in">exit</span></span><span> /b 0
)

REM ---------- Go ----------
</span><span><span class="hljs-keyword">if</span></span><span> /I </span><span><span class="hljs-string">"%EXT%"</span></span><span>==</span><span><span class="hljs-string">".go"</span></span><span> (
  </span><span><span class="hljs-built_in">where</span></span><span> /q go
  </span><span><span class="hljs-keyword">if</span></span><span> errorlevel 1 ( </span><span><span class="hljs-built_in">echo</span></span><span> [خطا] Go not found. Install Go SDK. &amp; pause &amp; </span><span><span class="hljs-built_in">exit</span></span><span> /b 2 )
  go build -o </span><span><span class="hljs-string">"%DIST%\%BASENAME%.exe"</span></span><span> </span><span><span class="hljs-string">"%SRC%"</span></span><span> &gt; </span><span><span class="hljs-string">"%DIST%\build.log"</span></span><span> 2&gt;&amp;1
  </span><span><span class="hljs-keyword">if</span></span><span> errorlevel 1 ( </span><span><span class="hljs-built_in">echo</span></span><span> [خطا] go build failed. See %DIST%\build.log &amp; pause ) </span><span><span class="hljs-keyword">else</span></span><span> ( </span><span><span class="hljs-built_in">echo</span></span><span> ✅ Output: %DIST%\%BASENAME%.exe &amp; start explorer </span><span><span class="hljs-string">"%DIST%"</span></span><span> )
  </span><span><span class="hljs-built_in">exit</span></span><span> /b 0
)

REM ---------- Rust ----------
</span><span><span class="hljs-keyword">if</span></span><span> /I </span><span><span class="hljs-string">"%EXT%"</span></span><span>==</span><span><span class="hljs-string">".rs"</span></span><span> (
  </span><span><span class="hljs-built_in">where</span></span><span> /q cargo
  </span><span><span class="hljs-keyword">if</span></span><span> errorlevel 1 ( </span><span><span class="hljs-built_in">echo</span></span><span> [خطا] cargo not found. Install Rust toolchain. &amp; pause &amp; </span><span><span class="hljs-built_in">exit</span></span><span> /b 2 )
  REM build single-file crate (requires Cargo project); fallback: rustc </span><span><span class="hljs-keyword">for</span></span><span> single file
  rustc </span><span><span class="hljs-string">"%SRC%"</span></span><span> -o </span><span><span class="hljs-string">"%DIST%\%BASENAME%.exe"</span></span><span> 2&gt; </span><span><span class="hljs-string">"%DIST%\build.log"</span></span><span>
  </span><span><span class="hljs-keyword">if</span></span><span> errorlevel 1 ( </span><span><span class="hljs-built_in">echo</span></span><span> [خطا] rustc failed. See %DIST%\build.log &amp; pause ) </span><span><span class="hljs-keyword">else</span></span><span> ( </span><span><span class="hljs-built_in">echo</span></span><span> ✅ Output: %DIST%\%BASENAME%.exe &amp; start explorer </span><span><span class="hljs-string">"%DIST%"</span></span><span> )
  </span><span><span class="hljs-built_in">exit</span></span><span> /b 0
)

REM ---------- C / C++ ----------
</span><span><span class="hljs-keyword">if</span></span><span> /I </span><span><span class="hljs-string">"%EXT%"</span></span><span>==</span><span><span class="hljs-string">".c"</span></span><span> (
  </span><span><span class="hljs-built_in">where</span></span><span> /q clang
  </span><span><span class="hljs-keyword">if</span></span><span> errorlevel 0 (
    clang </span><span><span class="hljs-string">"%SRC%"</span></span><span> -o </span><span><span class="hljs-string">"%DIST%\%BASENAME%.exe"</span></span><span> 2&gt; </span><span><span class="hljs-string">"%DIST%\build.log"</span></span><span>
  ) </span><span><span class="hljs-keyword">else</span></span><span> </span><span><span class="hljs-keyword">if</span></span><span> exist </span><span><span class="hljs-string">"%ProgramFiles(x86)%\Microsoft Visual Studio"</span></span><span> (
    </span><span><span class="hljs-built_in">echo</span></span><span> Trying cl (MSVC)...
    REM user must have Developer Command Prompt; we try cl
    cl </span><span><span class="hljs-string">"%SRC%"</span></span><span> /Fe:</span><span><span class="hljs-string">"%DIST%\%BASENAME%.exe"</span></span><span> &gt; </span><span><span class="hljs-string">"%DIST%\build.log"</span></span><span> 2&gt;&amp;1
  ) </span><span><span class="hljs-keyword">else</span></span><span> (
    </span><span><span class="hljs-built_in">where</span></span><span> /q gcc
    </span><span><span class="hljs-keyword">if</span></span><span> errorlevel 0 (
      gcc </span><span><span class="hljs-string">"%SRC%"</span></span><span> -o </span><span><span class="hljs-string">"%DIST%\%BASENAME%.exe"</span></span><span> 2&gt; </span><span><span class="hljs-string">"%DIST%\build.log"</span></span><span>
    ) </span><span><span class="hljs-keyword">else</span></span><span> (
      </span><span><span class="hljs-built_in">echo</span></span><span> [خطا] No C compiler found. Install MinGW or Visual Studio Build Tools.
      pause
      </span><span><span class="hljs-built_in">exit</span></span><span> /b 2
