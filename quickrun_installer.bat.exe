 build_to_exe.ps1
 Usage (internal via context menu): powershell -ExecutionPolicy Bypass -File "C:\DevTools\build_to_exe.ps1" "%1"
 This script detects file type and runs appropriate builder:
  - .py -&gt; PyInstaller (--onefile)
  - .ps1 -&gt; ps2exe (Invoke-PS2EXE) OR use PS2EXE-GUI's commandline
  - .bat/.cmd -&gt; wrap via 7-Zip SFX or Bat-To-Exe if installed
 Outputs placed in same folder as source under "dist\&lt;filename&gt;\"
#&gt;

param([string]$SourcePath)

if (-not $SourcePath) {
    Write-Error "No source file provided."
    exit 1
}

$SourcePath = (Resolve-Path $SourcePath).Path
$ext = [IO.Path]::GetExtension($SourcePath).ToLowerInvariant()
$base = [IO.Path]::GetFileNameWithoutExtension($SourcePath)
$dir = [IO.Path]::GetDirectoryName($SourcePath)
$distDir = Join-Path $dir "dist\$base"
New-Item -ItemType Directory -Path $distDir -Force | Out-Null

Write-Host "Building $SourcePath ..."

switch ($ext) {
    ".py" {
        # try locate python interpreter and pyinstaller
        $py = Get-Command py -ErrorAction SilentlyContinue
        if (-not $py) { $py = Get-Command python -ErrorAction SilentlyContinue }
        if (-not $py) { Write-Error "Python not found in PATH."; exit 2 }
        # install pyinstaller locally to user if missing
        try {
            &amp; $py.Path -m pip show pyinstaller &gt; $null 2&gt;&amp;1
        } catch {
            Write-Host "Installing pyinstaller to user site..."
            &amp; $py.Path -m pip install --user pyinstaller
        }
        Write-Host "Running PyInstaller..."
        &amp; $py.Path -m PyInstaller --onefile --distpath $distDir --workpath "$env:TEMP\pyinst_work" --specpath "$env:TEMP\pyinst_spec" $SourcePath
        Write-Host "PyInstaller finished. Check $distDir"
    }
    ".ps1" {
        # use ps2exe (module) if available
        $ps2exe = Get-Module -ListAvailable -Name ps2exe -ErrorAction SilentlyContinue
        if ($ps2exe) {
            Write-Host "Using ps2exe module..."
            Invoke-Command -ScriptBlock {
                param($s,$o)
                Import-Module ps2exe
                Invoke-ps2exe -inputFile $s -outputFile $o -noConsole -noProfile -icon ""
            } -ArgumentList $SourcePath,(Join-Path $distDir "$base.exe")
        } else {
            # try ps2exe.ps1 script if present in C:\DevTools
            $ps2exeScript = "C:\DevTools\ps2exe.ps1"
            if (Test-Path $ps2exeScript) {
                &amp; powershell -ExecutionPolicy Bypass -File $ps2exeScript -input $SourcePath -output (Join-Path $distDir "$base.exe")
            } else {
                Write-Error "ps2exe not found. Install PS2EXE module from PSGallery: Install-Module -Name ps2exe -Scope CurrentUser"
            }
        }
        Write-Host "PS script conversion finished. Check $distDir"
    }
    ".bat" | ".cmd" {
        # if Bat To Exe Converter CLI installed, use it; else use 7-Zip SFX approach
        $bat2exe = Get-Command BatToExeConverter -ErrorAction SilentlyContinue
        if ($bat2exe) {
            &amp; $bat2exe $SourcePath (Join-Path $distDir "$base.exe")
        } else {
            Write-Host "Creating self-extracting SFX installer (7-Zip) as fallback..."
            $tmp = Join-Path $env:TEMP ("sfxpack_" + [guid]::NewGuid().ToString("N"))
            New-Item -ItemType Directory -Path $tmp | Out-Null
            Copy-Item -Path $SourcePath -Destination (Join-Path $tmp (Split-Path $SourcePath -Leaf))
            Push-Location $tmp
            &amp; "C:\Program Files\7-Zip\7z.exe" a -t7z payload.7z * | Out-Null
            $sfx = "C:\Program Files\7-Zip\7z.sfx"
            $cfg = Join-Path $tmp "config.txt"
            "@
;!@Install@!UTF-8!
RunProgram=""$([IO.Path]::GetFileName($SourcePath))""
;!@InstallEnd@!
"@ | Out-File -FilePath $cfg -Encoding UTF8
            $out = Join-Path $distDir "$base.exe"
            cmd /c "copy /b `"$sfx`"+`"$cfg`"+`"payload.7z`" `"$out`""
            Pop-Location
            Remove-Item -Recurse -Force $tmp
            Write-Host "SFX created at $out"
        }
    }
    default {
        Write-Error "Unsupported file type: $ext"
        exit 10
    }
}

# Optionally open the dist folder
ii $distDir
</span></code></div></div></pre>
