# Run as Administrator

Set-StrictMode -Version Latest

Write-Host "Ø´Ø±ÙˆØ¹ Ù†ØµØ¨ Ø§Ø¨Ø²Ø§Ø±Ù‡Ø§... (Ù…Ù…Ú©Ù† Ø§Ø³Øª Ú†Ù†Ø¯ Ø¯Ù‚ÛŒÙ‚Ù‡ Ø·ÙˆÙ„ Ø¨Ú©Ø´Ø¯)" -ForegroundColor Cyan

# 1) Install core apps with winget (if winget not available, use manual installers)
Write-Host "Installing VSCode, Git, Python, Node.js LTS, Go, Rust toolchain prerequisites..." -ForegroundColor Yellow
winget install --id=Microsoft.VisualStudioCode -e --silent
winget install --id=Git.Git -e --silent
winget install --id=Python.Python.3 -e --silent
winget install --id=OpenJS.NodeJS.LTS -e --silent
winget install --id=Google.Go -e --silent
# Visual Studio Build Tools for MSVC (choose default workload)
winget install --id=Microsoft.VisualStudio.2022.BuildTools -e

# 2) Install Docker Desktop (optional)
Write-Host "Installing Docker Desktop (optional)..." -ForegroundColor Yellow
winget install --id=Docker.DockerDesktop -e

# 3) Install additional developer tools (CMake, Ninja, UPX)
winget install --id=Kitware.CMake -e --silent
winget install --id=Kitware.Ninja -e --silent
winget install --id=UPX.UPX -e --silent

# 4) Ensure 'code' command is available (VSCode)
$env:Path += ";$env:LOCALAPPDATA\Programs\Microsoft VS Code\bin"
if (-not (Get-Command code -ErrorAction SilentlyContinue)) {
    Write-Host "VSCode 'code' CLI not found in PATH. Please open VSCode -&gt; Command Palette -&gt; 'Shell Command: Install 'code' command in PATH' then re-run this script." -ForegroundColor Red
} else {
    Write-Host "'code' command available." -ForegroundColor Green
}

# 5) Install Python packages (PyInstaller, Nuitka) into user pip
Write-Host "Installing Python packaging tools (user-level)..." -ForegroundColor Yellow
python -m pip install --user --upgrade pip
python -m pip install --user pyinstaller nuitka

# 6) Install rustup (for Rust toolchain)
Write-Host "Installing rustup (Rust toolchain)..." -ForegroundColor Yellow
# runs rustup installer (non-interactive)
Invoke-Expression "&amp; { iwr https://sh.rustup.rs -UseBasicParsing } -OutVariable __tmp; iex ($__tmp | Out-String) -ArgumentList '-y'"

# 7) Install nvm and Node LTS (if you want nvm over system Node)
Write-Host "Installing nvm (optional)..." -ForegroundColor Yellow
# install nvm-windows alternative is manual; we already installed Node via winget

# 8) Install useful global npm tools (pkg, nexe) if npm available
if (Get-Command npm -ErrorAction SilentlyContinue) {
    npm install -g pkg nexe
}

# 9) Install VSCode extensions (use official extension identifiers)
$extensions = @(
    "ms-python.python",
    "ms-python.vscode-pylance",    # optional language server
    "ms-vscode.cpptools",
    "rust-lang.rust-analyzer",
    "golang.go",
    "eamodio.gitlens",
    "ms-azuretools.vscode-docker",
    "streetsidesoftware.code-spell-checker",
    "Codeium.codeium",            # Codeium (Windsurf) AI assistant
    "ms-vscode.vscode-node-azure-pack" # node helpers (optional)
)

Write-Host "Installing VSCode extensions..." -ForegroundColor Yellow
foreach ($ext in $extensions) {
    code --install-extension $ext --force
}

