/div></div></div><div class=3D"overflow-y-auto p-4" dir=3D"ltr"><code class=
=3D"whitespace-pre! language-bat"><span>@echo off
REM build_to_exe_with_icon.bat
setlocal enabledelayedexpansion

if "%~1"=3D=3D"" (
  echo No file provided.
  pause
  exit /b 1
)

set "SRC=3D%~1"
set "EXT=3D%~x1"
set "BASENAME=3D%~n1"
set "DIR=3D%~dp1"
set "DIST=3D%DIR%dist\%BASENAME%"
if not exist "%DIST%" mkdir "%DIST%"

REM find icon: 1) same-name .ico next to source, 2) default in C:\DevTools\=
icons\default.ico
set "ICON=3D"
if exist "%DIR%%BASENAME%.ico" (
  set "ICON=3D%DIR%%BASENAME%.ico"
) else if exist "C:\DevTools\icons\%BASENAME%.ico" (
  set "ICON=3DC:\DevTools\icons\%BASENAME%.ico"
) else if exist "C:\DevTools\icons\default.ico" (
  set "ICON=3DC:\DevTools\icons\default.ico"
)

echo Building %SRC% ...
if defined ICON (
  echo Using icon: %ICON%
) else (
  echo No icon found, building without custom icon
)

REM ---------- Python ----------
if /I "%EXT%"=3D=3D".py" (
  py -3 -m pip install --user --upgrade pip &gt;nul 2&gt;&amp;1
  py -3 -m pip install --user pyinstaller &gt;nul 2&gt;&amp;1
  if defined ICON (
    py -3 -m PyInstaller --onefile --icon "%ICON%" --distpath "%DIST%" --wo=
rkpath "%TEMP%\pyinst_work" --specpath "%TEMP%\pyinst_spec" "%SRC%" &gt; "%=
DIST%\build.log" 2&gt;&amp;1
  ) else (
    py -3 -m PyInstaller --onefile --distpath "%DIST%" --workpath "%TEMP%\p=
yinst_work" --specpath "%TEMP%\pyinst_spec" "%SRC%" &gt; "%DIST%\build.log"=
 2&gt;&amp;1
  )
  if errorlevel 1 ( echo [ERROR] PyInstaller failed. See %DIST%\build.log &=
amp; pause ) else ( echo =E2=9C=85 Output: "%DIST%\%BASENAME%.exe" &amp; st=
art explorer "%DIST%" )
  exit /b 0
)

REM ---------- PowerShell (.ps1) ----------
if /I "%EXT%"=3D=3D".ps1" (
  powershell -NoProfile -ExecutionPolicy Bypass -Command ^
    "Try { if (-not (Get-Module -ListAvailable -Name ps2exe)) { Install-Mod=
ule -Name ps2exe -Scope CurrentUser -Force } ; Import-Module ps2exe -ErrorA=
ction Stop; Invoke-ps2exe -inputFile '%SRC%' -outputFile '%DIST%\\%BASENAME=
%.exe' -icon '%ICON%' -noConsole } Catch { Write-Error $_ }" &gt; "%DIST%\b=
uild.log" 2&gt;&amp;1
  if errorlevel 1 ( echo [ERROR] ps2exe failed. See %DIST%\build.log &amp; =
pause ) else ( echo =E2=9C=85 Output: "%DIST%\%BASENAME%.exe" &amp; start e=
xplorer "%DIST%" )
  exit /b 0
)

REM ---------- Batch (.bat) ----------
if /I "%EXT%"=3D=3D".bat" (
  if exist "%ProgramFiles%\7-Zip\7z.exe" (
    set "TMP=3D%TEMP%\sfx_%BASENAME%"
    if exist "%TMP%" rd /s /q "%TMP%"
    mkdir "%TMP%"
    copy "%SRC%" "%TMP%" &gt;nul
    pushd "%TMP%"
    "%ProgramFiles%\7-Zip\7z.exe" a -t7z payload.7z * &gt; "%DIST%\7z.log" =
2&gt;&amp;1
    set "SFX=3D%ProgramFiles%\7-Zip\7z.sfx"
    copy /b "%SFX%"+ "payload.7z" "%DIST%\%BASENAME%.exe" &gt;nul
    popd
    rd /s /q "%TMP%"
    REM if icon available, try to set resource using rcedit (https://github=
.com/electron/rcedit)
    if defined ICON (
      if exist "%~dp0rcedit.exe" (
        "%~dp0rcedit.exe" "%DIST%\%BASENAME%.exe" --set-icon "%ICON%"
      ) else (
        echo [WARN] rcedit.exe not found next to script; icon not embedded.=
 To embed icon, download rcedit.exe and place next to this script.
      )
    )
    echo =E2=9C=85 Output: "%DIST%\%BASENAME%.exe"
    start explorer "%DIST%"
    exit /b 0
  ) else (
    echo [ERROR] 7-Zip not found. Install 7-Zip.
    pause
    exit /b 2
  )
)

REM ---------- Fallback ----------
echo Unsupported extension: %EXT%
pause
exit /b 10
