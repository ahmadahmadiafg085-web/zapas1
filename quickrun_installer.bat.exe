51 6.01057 5.53708 6.00551 6.00391 6.00391C6.00551 5.53708 6.01057 5.14051 6.03809 4.80371C6.0755 4.34588 6.15483 3.94012 6.34668 3.56348L6.46875 3.34473C6.77282 2.84912 7.20856 2.44514 7.72949 2.17969L7.87207 2.11328C8.20855 1.96886 8.56979 1.90385 8.9707 1.87109C9.42091 1.83432 9.978 1.83496 10.667 1.83496H13.5C14.1891 1.83496 14.746 1.83431 15.1963 1.87109C15.6541 1.90851 16.0599 1.98788 16.4365 2.17969L16.6553 2.30176C17.151 2.60585 17.5548 3.04244 17.8203 3.56348L17.8867 3.70606C18.031 4.04235 18.0962 4.40306 18.1289 4.80371C18.1657 5.25395 18.165 5.81091 18.165 6.5V9.33301Z"></path></svg>Copy code</button></div></div></div><div class="overflow-y-auto p-4" dir="ltr"><code class="whitespace-pre! language-powershell"><span>&lt;#
package_into_vhd.ps1
Run as Administrator.
Usage: .\package_into_vhd.ps1 "C:\path\to\your\file.exe" -RunAfterExtract:$true
This script creates a VHDX, mounts it, copies the file into it, optionally runs the file, then dismounts.
#&gt;

param(
    [Parameter(Mandatory=$true)]
    [string]$SourcePath,

    [bool]$RunAfterExtract = $false,

    [string]$VhdPath = "$env:SystemDrive\DevPackage.vhdx",
    [int]$VhdSizeGB = 5,
    [string]$VolumeLabel = "DevPackage"
)

if (-not (Test-Path $SourcePath)) {
    Write-Error "Source file not found: $SourcePath"
    exit 1
}

if (-not ([Security.Principal.WindowsPrincipal][Security.Principal.WindowsIdentity]::GetCurrent()).IsInRole([Security.Principal.WindowsBuiltinRole]::Administrator)) {
    Write-Error "Run this script as Administrator."
    exit 2
}

# Create or reuse VHD
if (-not (Test-Path $VhdPath)) {
    Write-Host "Creating VHD: $VhdPath ($VhdSizeGB GB)..."
    New-VHD -Path $VhdPath -SizeBytes (${VhdSizeGB}GB) -Dynamic | Out-Null
} else {
    Write-Host "VHD already exists, it will be reused."
}

# Mount
$mounted = Mount-VHD -Path $VhdPath -Passthru

Start-Sleep -Seconds 1

# Find disk and partition
$disk = Get-Disk | Where-Object { $_.UniqueId -eq $mounted.DiskNumber -or $_.Number -eq $mounted.DiskNumber } -ErrorAction SilentlyContinue
if (-not $disk) {
    # fallback: try to find by size
    $vhdSize = (Get-Item $VhdPath).Length
    $disk = Get-Disk | Where-Object { $_.Size -eq $vhdSize } | Select-Object -First 1
}

if (-not $disk) {
    Write-Error "Could not identify disk for the mounted VHD. Unmounting and exiting."
    Dismount-VHD -Path $VhdPath -ErrorAction SilentlyContinue
    exit 3
}

# Initialize and format if RAW
if ($disk.PartitionStyle -eq 'RAW' -or $disk.PartitionStyle -eq 'Unknown') {
    Write-Host "Initializing disk and creating partition..."
    Initialize-Disk -Number $disk.Number -PartitionStyle MBR -PassThru | Out-Null
    $part = New-Partition -DiskNumber $disk.Number -UseMaximumSize -AssignDriveLetter
    Format-Volume -Partition $part -FileSystem NTFS -NewFileSystemLabel $VolumeLabel -Confirm:$false | Out-Null
} else {
    $part = Get-Partition -DiskNumber $disk.Number | Select-Object -First 1
}

$vol = Get-Volume -Partition $part
$drive = "$($vol.DriveLetter):\"

# Copy file into VHD
$dest = Join-Path $drive (Split-Path $SourcePath -Leaf)
Write-Host "Copying $SourcePath -&gt; $dest"
Copy-Item -Path $SourcePath -Destination $dest -Force

# Optionally run the file inside mounted drive
if ($RunAfterExtract) {
    Write-Host "Running $dest ..."
    Start-Process -FilePath $dest -WorkingDirectory (Split-Path $dest) -Wait
}

# Unmount/dismount
Write-Host "Dismounting VHD..."
Dismount-VHD -Path $VhdPath

Write-Host "Done. VHD created at: $VhdPath (it was mounted while copying)."
</span></code></div></div></pre>
