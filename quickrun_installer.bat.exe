REM build_to_exe.bat (نسخه با نمایش مسیر خروجی)
setlocal enabledelayedexpansion

if "%~1"=="" (
  echo [خطا] هيچ فايلي انتخاب نشده.
  pause
  exit /b 1
)

set "SRC=%~1"
set "EXT=%~x1"
set "BASENAME=%~n1"
set "DIR=%~dp1"
set "DIST=%DIR%dist\%BASENAME%"
if not exist "%DIST%" mkdir "%DIST%"

echo ================================
echo در حال ساخت فايل: %SRC%
echo ================================

REM --- Python (.py) ---
if /I "%EXT%"==".py" (
  pyinstaller --onefile "%SRC%" --distpath "%DIST%" --workpath "%TEMP%\pyinst_work" --specpath "%TEMP%\pyinst_spec" &gt;"%DIST%\build.log" 2&gt;&amp;1
  if errorlevel 1 (
    echo [خطا] PyInstaller اجرا نشد. فايل لاگ: %DIST%\build.log
    pause
  ) else (
    echo.
    echo ✅ خروجي ساخته شد:
    echo    %DIST%\%BASENAME%.exe
    start explorer "%DIST%"
  )
  exit /b 0
)

REM --- PowerShell (.ps1) ---
if /I "%EXT%"==".ps1" (
  powershell -NoProfile -ExecutionPolicy Bypass -Command ^
    "Import-Module ps2exe -ErrorAction Stop; Invoke-ps2exe -inputFile '%SRC%' -outputFile '%DIST%\\%BASENAME%.exe' -noConsole" &gt;"%DIST%\build.log" 2&gt;&amp;1
  if errorlevel 1 (
    echo [خطا] ps2exe نصب نيست يا شكست خورد. لاگ: %DIST%\build.log
    pause
  ) else (
    echo.
    echo ✅ خروجي ساخته شد:
    echo    %DIST%\%BASENAME%.exe
    start explorer "%DIST%"
  )
  exit /b 0
)

REM --- Batch (.bat/.cmd) ---
if /I "%EXT%"==".bat" (
  if exist "%ProgramFiles%\7-Zip\7z.exe" (
    set "TMP=%TEMP%\sfx_%BASENAME%"
    if exist "%TMP%" rd /s /q "%TMP%"
    mkdir "%TMP%"
    copy "%SRC%" "%TMP%" &gt;nul
    pushd "%TMP%"
    "%ProgramFiles%\7-Zip\7z.exe" a -t7z payload.7z * &gt;nul
    set "SFX=%ProgramFiles%\7-Zip\7z.sfx"
    echo ;!@Install@!UTF-8!&gt;"config.txt"
    echo RunProgram="%~nx1"&gt;&gt;"config.txt"
    echo ;!@InstallEnd@!&gt;&gt;"config.txt"
    copy /b "%SFX%"+ "config.txt" + "payload.7z" "%DIST%\%BASENAME%.exe" &gt;nul
    popd
    rd /s /q "%TMP%"
    echo.
    echo ✅ خروجي ساخته شد:
    echo    %DIST%\%BASENAME%.exe
    start explorer "%DIST%"
    exit /b 0
  ) else (
    echo [خطا] 7-Zip نصب نيست (مسير: C:\Program Files\7-Zip\7z.exe)
    pause
    exit /b 2
  )
)

echo [خطا] پسوند پشتيباني نمي‌شود: %EXT%
pause
exit /b 10
</span></code></div></div></pre>
