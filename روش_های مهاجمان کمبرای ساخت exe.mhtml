lass=3D"whitespace-pre! language-powershell"><span>&lt;#
package_into_vhd.ps1
Run as Administrator.
Usage: .\package_into_vhd.ps1 "C:\path\to\your\file.exe" -RunAfterExtract:$=
true
This script creates a VHDX, mounts it, copies the file into it, optionally =
runs the file, then dismounts.
#&gt;

param(
    [Parameter(Mandatory=3D$true)]
    [string]$SourcePath,

    [bool]$RunAfterExtract =3D $false,

    [string]$VhdPath =3D "$env:SystemDrive\DevPackage.vhdx",
    [int]$VhdSizeGB =3D 5,
    [string]$VolumeLabel =3D "DevPackage"
)

if (-not (Test-Path $SourcePath)) {
    Write-Error "Source file not found: $SourcePath"
    exit 1
}

if (-not ([Security.Principal.WindowsPrincipal][Security.Principal.WindowsI=
dentity]::GetCurrent()).IsInRole([Security.Principal.WindowsBuiltinRole]::A=
dministrator)) {
    Write-Error "Run this script as Administrator."
    exit 2
}

# Create or reuse VHD
if (-not (Test-Path $VhdPath)) {
    Write-Host "Creating VHD: $VhdPath ($VhdSizeGB GB)..."
    New-VHD -Path $VhdPath -SizeBytes (${VhdSizeGB}GB) -Dynamic | Out-Null
} else {
    Write-Host "VHD already exists, it will be reused."
}

# Mount
$mounted =3D Mount-VHD -Path $VhdPath -Passthru

Start-Sleep -Seconds 1

# Find disk and partition
$disk =3D Get-Disk | Where-Object { $_.UniqueId -eq $mounted.DiskNumber -or=
 $_.Number -eq $mounted.DiskNumber } -ErrorAction SilentlyContinue
if (-not $disk) {
    # fallback: try to find by size
    $vhdSize =3D (Get-Item $VhdPath).Length
    $disk =3D Get-Disk | Where-Object { $_.Size -eq $vhdSize } | Select-Obj=
ect -First 1
}

if (-not $disk) {
    Write-Error "Could not identify disk for the mounted VHD. Unmounting an=
d exiting."
    Dismount-VHD -Path $VhdPath -ErrorAction SilentlyContinue
    exit 3
}

# Initialize and format if RAW
if ($disk.PartitionStyle -eq 'RAW' -or $disk.PartitionStyle -eq 'Unknown') =
{
    Write-Host "Initializing disk and creating partition..."
    Initialize-Disk -Number $disk.Number -PartitionStyle MBR -PassThru | Ou=
t-Null
    $part =3D New-Partition -DiskNumber $disk.Number -UseMaximumSize -Assig=
nDriveLetter
    Format-Volume -Partition $part -FileSystem NTFS -NewFileSystemLabel $Vo=
lumeLabel -Confirm:$false | Out-Null
} else {
    $part =3D Get-Partition -DiskNumber $disk.Number | Select-Object -First=
 1
}

$vol =3D Get-Volume -Partition $part
$drive =3D "$($vol.DriveLetter):\"

# Copy file into VHD
$dest =3D Join-Path $drive (Split-Path $SourcePath -Leaf)
Write-Host "Copying $SourcePath -&gt; $dest"
Copy-Item -Path $SourcePath -Destination $dest -Force

# Optionally run the file inside mounted drive
if ($RunAfterExtract) {
    Write-Host "Running $dest ..."
    Start-Process -FilePath $dest -WorkingDirectory (Split-Path $dest) -Wai=
t
}

# Unmount/dismount
Write-Host "Dismounting VHD..."
Dismount-VHD -Path $VhdPath

Write-Host "Done. VHD created at: $VhdPath (it was mounted while copying)."
