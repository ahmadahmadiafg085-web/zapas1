"whitespace-pre! language-bat"><span>@echo off
REM build_to_exe.bat
REM Usage: Right-click a file -&gt; Build to EXE (registered to this script=
)
REM This script must be at C:\DevTools\build_to_exe.bat for the provided .r=
eg to work.
setlocal enabledelayedexpansion

if "%~1"=3D=3D"" (
  echo No file provided.
  pause
  exit /b 1
)

set "SRC=3D%~1"
set "EXT=3D%~x1"
set "BASENAME=3D%~n1"
set "DIR=3D%~dp1"
set "DIST=3D%DIR%dist\%BASENAME%"
if not exist "%DIST%" mkdir "%DIST%"

echo Building %SRC% ...
echo.

REM --- Python ---
if /I "%EXT%"=3D=3D".py" (
  REM use py or python -m pyinstaller
  if exist "%ProgramFiles%\Python\Python37\python.exe" (
    REM nothing, prefer system py
  )
  echo Running PyInstaller...
  pyinstaller --onefile "%SRC%" --distpath "%DIST%" --workpath "%TEMP%\pyin=
st_work" --specpath "%TEMP%\pyinst_spec" &gt;"%DIST%\build.log" 2&gt;&amp;1
  if errorlevel 1 (
    echo PyInstaller failed. See %DIST%\build.log
    pause
  ) else (
    echo Done. Output: %DIST%\%BASENAME%.exe
    explorer "%DIST%"
  )
  exit /b 0
)

REM --- PowerShell (.ps1) ---
if /I "%EXT%"=3D=3D".ps1" (
  REM Try using ps2exe module (Invoke-ps2exe) via PowerShell
  echo Converting PowerShell script to EXE...
  powershell -NoProfile -ExecutionPolicy Bypass -Command ^
    "Try { if (-not (Get-Module -ListAvailable -Name ps2exe)) { Write-Host =
'ps2exe module not found. Please Install-Module ps2exe -Scope CurrentUser' =
; exit 2 } ; Import-Module ps2exe; Invoke-ps2exe -inputFile '%SRC%' -output=
File '%DIST%\\%BASENAME%.exe' -noConsole } Catch { Write-Host 'Error: ' $_;=
 exit 3 }" &gt; "%DIST%\build.log" 2&gt;&amp;1
  if errorlevel 1 (
    echo ps2exe failed or not installed. See %DIST%\build.log
    pause
  ) else (
    echo Done. Output: %DIST%\%BASENAME%.exe
    explorer "%DIST%"
  )
  exit /b 0
)

REM --- Batch (.bat/.cmd) ---
if /I "%EXT%"=3D=3D".bat" (
  echo Building SFX for batch file using 7-Zip...
  REM temp working dir
  set "TMP=3D%TEMP%\sfx_%BASENAME%"
  if exist "%TMP%" rd /s /q "%TMP%"
  mkdir "%TMP%"
  copy "%SRC%" "%TMP%" &gt;nul
  pushd "%TMP%"
  if exist "%ProgramFiles%\7-Zip\7z.exe" (
    "%ProgramFiles%\7-Zip\7z.exe" a -t7z payload.7z * &gt; "%TMP%\7z.log" 2=
&gt;&amp;1
    set "SFX=3D%ProgramFiles%\7-Zip\7z.sfx"
    echo ;!@Install@!UTF-8!&gt;"config.txt"
    echo RunProgram=3D"%~nx1"&gt;&gt;"config.txt"
    echo ;!@InstallEnd@!&gt;&gt;"config.txt"
    copy /b "%SFX%"+ "config.txt" + "payload.7z" "%DIST%\%BASENAME%.exe" &g=
t;nul
    popd
    rd /s /q "%TMP%"
    echo Done. Output: %DIST%\%BASENAME%.exe
    explorer "%DIST%"
    exit /b 0
  ) else (
    popd
    rd /s /q "%TMP%"
    echo 7-Zip not found in %ProgramFiles%\7-Zip. Install 7-Zip or use anot=
her tool.
    pause
    exit /b 2
  )
)

echo Unsupported file extension: %EXT%
pause
exit /b 10
