ton></div></div></div><div class=3D"overflow-y-auto p-4" dir=3D"ltr"><code =
class=3D"whitespace-pre! language-powershell"><span>&lt;#
 build_to_exe.ps1
 Usage (internal via context menu): powershell -ExecutionPolicy Bypass -Fil=
e "C:\DevTools\build_to_exe.ps1" "%1"
 This script detects file type and runs appropriate builder:
  - .py -&gt; PyInstaller (--onefile)
  - .ps1 -&gt; ps2exe (Invoke-PS2EXE) OR use PS2EXE-GUI's commandline
  - .bat/.cmd -&gt; wrap via 7-Zip SFX or Bat-To-Exe if installed
 Outputs placed in same folder as source under "dist\&lt;filename&gt;\"
#&gt;

param([string]$SourcePath)

if (-not $SourcePath) {
    Write-Error "No source file provided."
    exit 1
}

$SourcePath =3D (Resolve-Path $SourcePath).Path
$ext =3D [IO.Path]::GetExtension($SourcePath).ToLowerInvariant()
$base =3D [IO.Path]::GetFileNameWithoutExtension($SourcePath)
$dir =3D [IO.Path]::GetDirectoryName($SourcePath)
$distDir =3D Join-Path $dir "dist\$base"
New-Item -ItemType Directory -Path $distDir -Force | Out-Null

Write-Host "Building $SourcePath ..."

switch ($ext) {
    ".py" {
        # try locate python interpreter and pyinstaller
        $py =3D Get-Command py -ErrorAction SilentlyContinue
        if (-not $py) { $py =3D Get-Command python -ErrorAction SilentlyCon=
tinue }
        if (-not $py) { Write-Error "Python not found in PATH."; exit 2 }
        # install pyinstaller locally to user if missing
        try {
            &amp; $py.Path -m pip show pyinstaller &gt; $null 2&gt;&amp;1
        } catch {
            Write-Host "Installing pyinstaller to user site..."
            &amp; $py.Path -m pip install --user pyinstaller
        }
        Write-Host "Running PyInstaller..."
        &amp; $py.Path -m PyInstaller --onefile --distpath $distDir --workp=
ath "$env:TEMP\pyinst_work" --specpath "$env:TEMP\pyinst_spec" $SourcePath
        Write-Host "PyInstaller finished. Check $distDir"
    }
    ".ps1" {
        # use ps2exe (module) if available
        $ps2exe =3D Get-Module -ListAvailable -Name ps2exe -ErrorAction Sil=
entlyContinue
        if ($ps2exe) {
            Write-Host "Using ps2exe module..."
            Invoke-Command -ScriptBlock {
                param($s,$o)
                Import-Module ps2exe
                Invoke-ps2exe -inputFile $s -outputFile $o -noConsole -noPr=
ofile -icon ""
            } -ArgumentList $SourcePath,(Join-Path $distDir "$base.exe")
        } else {
            # try ps2exe.ps1 script if present in C:\DevTools
            $ps2exeScript =3D "C:\DevTools\ps2exe.ps1"
            if (Test-Path $ps2exeScript) {
                &amp; powershell -ExecutionPolicy Bypass -File $ps2exeScrip=
t -input $SourcePath -output (Join-Path $distDir "$base.exe")
            } else {
                Write-Error "ps2exe not found. Install PS2EXE module from P=
SGallery: Install-Module -Name ps2exe -Scope CurrentUser"
            }
        }
        Write-Host "PS script conversion finished. Check $distDir"
    }
    ".bat" | ".cmd" {
        # if Bat To Exe Converter CLI installed, use it; else use 7-Zip SFX=
 approach
        $bat2exe =3D Get-Command BatToExeConverter -ErrorAction SilentlyCon=
tinue
        if ($bat2exe) {
            &amp; $bat2exe $SourcePath (Join-Path $distDir "$base.exe")
        } else {
            Write-Host "Creating self-extracting SFX installer (7-Zip) as f=
allback..."
            $tmp =3D Join-Path $env:TEMP ("sfxpack_" + [guid]::NewGuid().To=
String("N"))
            New-Item -ItemType Directory -Path $tmp | Out-Null
            Copy-Item -Path $SourcePath -Destination (Join-Path $tmp (Split=
-Path $SourcePath -Leaf))
            Push-Location $tmp
            &amp; "C:\Program Files\7-Zip\7z.exe" a -t7z payload.7z * | Out=
-Null
            $sfx =3D "C:\Program Files\7-Zip\7z.sfx"
            $cfg =3D Join-Path $tmp "config.txt"
            "@
;!@Install@!UTF-8!
RunProgram=3D""$([IO.Path]::GetFileName($SourcePath))""
;!@InstallEnd@!
"@ | Out-File -FilePath $cfg -Encoding UTF8
            $out =3D Join-Path $distDir "$base.exe"
            cmd /c "copy /b `"$sfx`"+`"$cfg`"+`"payload.7z`" `"$out`""
            Pop-Location
            Remove-Item -Recurse -Force $tmp
            Write-Host "SFX created at $out"
        }
    }
    default {
        Write-Error "Unsupported file type: $ext"
        exit 10
    }
}

# Optionally open the dist folder
ii $distDir
